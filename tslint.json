{
  "extends": "tslint:recommended",
  "rules": {
    "ordered-imports": [
      true,
      {
        "import-sources-order": "case-insensitive",
        "named-imports-order": "case-insensitive"
      }
    ],
    "interface-name": [false],
    "no-unused-variable": [true],
    "no-console": [false]
  },
  "jsRules": {
    "ordered-imports": [
      true,
      {
        "import-sources-order": "case-insensitive",
        "named-imports-order": "case-insensitive"
      }
    ]
  }
}
